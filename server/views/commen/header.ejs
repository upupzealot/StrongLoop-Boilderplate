(function() {
  var css = {};
  include_css = function(path) {
    if(!css[path]) {
      css[path] = true;
      return '<link rel="stylesheet" href="' + path + '"/>';
    }
  }

  var js = {}
  include_js = function(path) {
    if(!js[path]) {
      js[path] = true;
      return '<script src="' + path + '"></script>';
    }
  }
})()
%>

